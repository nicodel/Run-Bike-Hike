{
  "name": "runbikehike",
  "version": "0.2.0",
  "description": "Run, Bike, Hike... allow you to record your running or biking sessions, so you can follow your sporting progress.",
  "main": "server.js",
  "scripts": {
    "check:js": "./scripts/check-js.sh",
    "build": "./scripts/build.sh",
    "dev": "NODE_ENV=dev nodemon --debug ./server.js",
    "start": "NODE_ENV=prod node ./server.js",
    "test": "mocha test/tests.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "./scripts/verify.sh && npm run test/tests.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicodel/Run-Bike-Hike.git"
  },
  "author": "Nicolas Delebecque",
  "license": "MPL2.0 licence",
  "bugs": {
    "url": "https://github.com/nicodel/Run-Bike-Hike/issues"
  },
  "cozy-displayName": "Run, Bike, Hike...",
  "icon-path": "img/icon128.png",
  "dependencies": {
    "americano": "^0.4.2",
    "backbone.nativeajax": "^0.4.1",
    "backbone.nativeview": "^0.3.3",
    "body-parser": "^1.13.2",
    "cozydb": "0.1.3",
    "crossfilter": "^1.3.12",
    "d3": "^3.5.6",
    "dc": "^2.0.0-beta.14",
    "exoskeleton": "^0.7.0",
    "express": "^4.13.1",
    "microtemplates": "^0.1.0",
    "pouchdb": "5.1.0",
    "ejs": "^2.3.4",
    "webl10n": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "should": "^7.0.4",
    "uglifycss": "0.0.19",
    "csslint": "^0.10.0",
    "ghooks": "^1.0.1",
    "jshint": "^2.9.1-rc1",
    "uglify-js": "^2.6.1"
  }
}
